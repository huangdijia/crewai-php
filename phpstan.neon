parameters:
    level: 8
    paths:
        - src
        - tests
    excludePaths:
        - vendor
    ignoreErrors:
        - '#Call to an undefined method#'
        - '#Access to an undefined property#'
        -
            identifier: missingType.iterableValue
        -
            identifier: missingType.generics
    parallel:
        jobSize: 20
        maximumNumberOfProcesses: 32
        minimumNumberOfJobsPerProcess: 2
